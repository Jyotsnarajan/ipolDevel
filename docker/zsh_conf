##################
#     Zsh        #
##################
export ZSH=/home/ipol/.oh-my-zsh

ZSH_THEME="jtriley"

plugins=(
  git
)

source $ZSH/oh-my-zsh.sh

##################
#     Alias      #
##################

# IPOL Terminal 
alias terminal="cd ~/ipolDevel/tools/terminal && ./terminal.py"
# Control Panel
alias cp="cd ~/ipolDevel/ipol_webapp && python manage.py runserver 127.0.0.1:8000"

# Collec all static files from CP.
alias collectstatic="~/ipolDevel/ipol_webapp && python manage.py collectstatic --noinput --clear"


alias core="cd ~/ipolDevel/ipol_demo/modules/core && python main.py"
alias conversion="cd ~/ipolDevel/ipol_demo/modules/conversion && python main.py"
alias blobs="cd ~/ipolDevel/ipol_demo/modules/blobs && python main.py"
alias dispatcher="cd ~/ipolDevel/ipol_demo/modules/dispatcher && python main.py"
alias demorunner="cd ~/ipolDevel/ipol_demo/modules/demorunner && python main.py"
alias demoinfo="cd ~/ipolDevel/ipol_demo/modules/demoinfo && python main.py"
alias archive="cd ~/ipolDevel/ipol_demo/modules/archive && python main.py"